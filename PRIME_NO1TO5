// Online C++ compiler to run C++ program online
#include <iostream>
using namespace std;
int main() {
    // Write C++ code here
    int n=5;
    int i,j;
    for(i=2;i<=n;i++){
        int s=0;
        for(j=2;j<i;j++){
            if(i%j==0)
            s=+1;
            break;
        }    
    if(s==0){
    cout<<i;
    }
    cout<<endl;
}
    return 0;
}
